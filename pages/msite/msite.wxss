.search{
  padding: 30rpx;
  background: #fff;
}
.input-search{
  padding: 0 10rpx;
  height: 60rpx;
  background: #f3f5f9;
  font-size: 28rpx;
}

.msite .panel .title{
  background: #f3f5f9;
  color: #333;
  line-height: 60rpx;
  padding: 0 30rpx;
}
.msite .panel .address{
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  line-height: 80rpx;
  color: #0085ff;
}

.msite .panel .search-address{
  border-bottom: 1px solid #eee;
  padding: 20rpx 0;
  margin: 0 30rpx;
  color: #333;
}
.msite .panel .search-address .detail{
  color: #999;
  font-size: 24rpx;
}